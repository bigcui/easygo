#!/usr/bin/env node

const cli = require('../lib/cli');

const args = cli.setup({
    usage: '<element-name>',
    options: [
    ],
    help: [
        '    Examples:',
        cli.chalk.gray('    # 验证一个easygo组件'),
        '        $ easygo validateement easygo-**',
    ].join('\n')
});

let fileName = args[0];
// 检查模板更新后启动命令

    require('../lib/validate-element').exec({
        file: fileName
    });



