#!/usr/bin/env node

const cli = require('../lib/cli');

const args = cli.setup({
    noArgs: true,
    options: [
        ['-f, --force', '强制初始化'],
        ['-c, --custom', '初始化定制化 ara']
    ],
    help: [
        '初始化ara页面项目',
        '    $ ara init',
    ].join('\n')
});

require('../lib/validate').exec({
    file:args[0]
})
