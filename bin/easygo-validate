#!/usr/bin/env node

const cli = require('../lib/cli');

const args = cli.setup({
    usage: '<page-name>',
    options: [
    ],
    help: [
        '    Examples:',
        cli.chalk.gray('    # 验证easygo页面'),
        '        $ easygo validate page.html',
    ].join('\n')
});

let fileName = args[0];
    require('../lib/validate').exec({
        file: fileName
    });



